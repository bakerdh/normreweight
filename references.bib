	keywords = {Monocular patching; contrast-gain; eye dominance plasticity; intrinsic plasticity; steady-state visual evoked potentials; visual cortex},
	mesh = {Adult; Dominance, Ocular; Evoked Potentials, Visual; Female; Humans; Male; Neuronal Plasticity; Sensory Deprivation; Vision, Monocular; Visual Cortex; Young Adult},
