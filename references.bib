	mesh = {Adolescent; Adult; Afterimage; Color Perception; Humans; Models, Neurological; Motion Perception; Orientation; Time Factors},
	month = {Nov},
	number = {4},
	pages = {323-7},
	pmid = {1185119},
	pst = {ppublish},
	title = {Extremely long-term persistence of the McCollough effect},
	volume = {1},
