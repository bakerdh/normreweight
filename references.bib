	mesh = {Animals; Arousal; Autistic Disorder; Evoked Potentials; Humans; Models, Neurological; Neocortex; Nerve Net; Neural Inhibition; Neural Pathways; Neurotransmitter Agents; Rats},
	month = {Oct},
	number = {5},
