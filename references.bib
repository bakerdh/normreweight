	journal = {Neuroimage},
	journal-full = {NeuroImage},
	mesh = {Adolescent; Age Factors; Algorithms; Brain; Child; Child, Preschool; Databases, Factual; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Observer Variation; Pediatrics; Reference Values},
	month = {Jan},
