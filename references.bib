	keywords = {Bayesian; adaptation; autism; number; prediction},
	mesh = {Adaptation, Psychological; Child; Child Development Disorders, Pervasive; Female; Humans; Male},
	month = {Jun},
	number = {25},
	pages = {7868-72},
