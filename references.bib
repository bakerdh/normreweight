	mesh = {Action Potentials; Adaptation, Physiological; Animals; Cats; Geniculate Bodies; Models, Neurological; Neural Inhibition; Neurons; Photic Stimulation; Synaptic Transmission; Visual Cortex; Visual Pathways; Visual Perception},
	month = {Aug},
